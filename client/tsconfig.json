{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": ["*", "src/*"]
    },
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./src", "./node_modules/@types"],
    "types": ["node"],
    "module": "esnext",
  },
  "include": ["src"]
}
